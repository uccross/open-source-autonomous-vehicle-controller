{
  "name": "@jcamp/tailwindcss-plugin-icons",
  "version": "0.5.0",
  "description": "Automatic icon creator for Tailwind",
  "packageManager": "pnpm@8.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "John Campion (https://github.com/JohnCampionJr/)",
  "repository": "https://github.com/jcamp-code/tailwindcss-plugin-icons",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@iconify/types": "^2.0.0",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "c8": "^7.13.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.3.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest/customMatchers.js"
    ]
  },
  "c8": {
    "exclude": [
      "jest/customMatchers.js",
      "**/*.test.js"
    ]
  },
  "keywords": [
    "tailwindcss",
    "iconify",
    "icons"
  ],
  "dependencies": {
    "@iconify/json": "2.2.43",
    "@iconify/utils": "^2.1.5"
  },
  "scripts": {
    "dev": "tsc --watch & cd ./demo && pnpm run-script dev",
    "build": "tsc",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "test": "tsc && c8 jest"
  }
}